1 REM 2ND DEGREE EQUATION SOLUTION (ONLY REAL ROOTS)
2 REM BY ARM_CODER 2025
10 PRINT "Coefficient A:"
20 INPUT A
30 PRINT "Coefficient B:"
40 INPUT B
50 PRINT "Coefficient C:"
60 INPUT C
70 PRINT "A:"; : PRINT A
80 PRINT "B:"; : PRINT B
90 PRINT "C:"; : PRINT C
100 D = B*B - 4 * A * C
110 PRINT "Delta:"; : PRINT D
120 IF D < 0 THEN PRINT "No real roots, sorry": END
130 R=SQR(D)
140 PRINT "Root 1:";
150 PRINT (-B+R)/(2*A)
160 PRINT "Root 2:";
170 PRINT (-B-R)/(2*A)
180 END

